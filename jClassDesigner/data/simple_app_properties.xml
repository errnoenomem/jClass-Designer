<?xml version="1.0" encoding="UTF-8"?>
<!--
    This XML file allows us to easily customize the look and language used
    in our program's user interface. Note that this properties file is only
    for the standard controls for an app from the SimpleAppFramework, so
    it would not include things initialized for the workspace, which is
    customizable.
-->
<properties>
    <property_list>
        <property name="APP_TITLE"	    value="jClass Designer"></property>
        <property name="APP_LOGO"	    value="jClass.png"></property>
        <property name="APP_PATH_CSS"	    value="css/"></property>
        <property name="APP_CSS"	    value="jcs_style.css"></property>
        
        <!-- PROGRAM ICON FILES -->
        <property name="NEW_ICON"	    value="New.png"></property>
        <property name="LOAD_ICON"	    value="Load.png"></property>
        <property name="SAVE_ICON"	    value="Save.png"></property>
        <property name="SAVEAS_ICON"	    value="SaveAs.png"></property>
        <property name="PHOTO_ICON"	    value="Photo.png"></property>
        <property name="CODE_ICON"	    value="Code.png"></property>
        <property name="EXIT_ICON"	    value="Exit.png"></property>
        <property name="SELECT_ICON"          value="Select.png"></property> 
        <property name="RESIZE_ICON"          value="Resize.png"></property> 
        <property name="ADDCLASS_ICON"          value="AddClass.png"></property> 
        <property name="ADDINTERFACE_ICON"          value="AddInterface.png"></property> 
        <property name="REMOVE_ICON"          value="Remove.png"></property> 
        <property name="UNDO_ICON"          value="Undo.png"></property> 
        <property name="REDO_ICON"          value="Redo.png"></property> 
        <property name="ZOOMIN_ICON"          value="ZoomIn.png"></property>
        <property name="ZOOMOUT_ICON"          value="ZoomOut.png"></property>
        
        <!-- TOOLTIPS FOR BUTTONS -->
        <property name="NEW_TOOLTIP"		value="Create a Blank Space"></property>
        <property name="LOAD_TOOLTIP"		value="Load an Existing Pose"></property>
        <property name="SAVE_TOOLTIP"		value="Save your work"></property>
        <property name="SAVEAS_TOOLTIP"		value="Save your work to a specified location"></property>
        <property name="EXPORTPHOTO_TOOLTIP"	value="Export as a photo"></property>
        <property name="EXPORTCODE_TOOLTIP"	value="Export as code"></property>
        <property name="EXIT_TOOLTIP"           value="Exit the application"></property>
        <property name="SELECT_TOOLTIP"		value="Select an Object"></property>
        <property name="RESIZE_TOOLTIP"		value="Resize an Object"></property>
        <property name="ADDCLASS_TOOLTIP"	value="Add a Class"></property>
        <property name="ADDINTERFACE_TOOLTIP"	value="Add an Interface"></property>
        <property name="REMOVE_TOOLTIP"		value="Remove the selected Object"></property>
        <property name="UNDO_TOOLTIP"		value="Undo previous action"></property>
        <property name="REDO_TOOLTIP"		value="Redo the action"></property>
        <property name="ZOOMIN_TOOLTIP"		value="Zooms in on the design"></property>
        <property name="ZOOMOUT_TOOLTIP"	value="Zoom out of the design"></property>
        
        <!-- ERROR MESSAGES -->
        <property name="CLOSE_BUTTON_TEXT"              value="Close"></property>

        <property name="NEW_ERROR_MESSAGE"              value="An error occured creating New work."></property>
        <property name="NEW_ERROR_TITLE"		value="New Work Error"></property>
        <property name="LOAD_ERROR_MESSAGE"             value="An error occured loading work."></property>
        <property name="LOAD_ERROR_TITLE"		value="Load Work Error"></property>
        <property name="SAVE_ERROR_MESSAGE"             value="An error occured saving work."></property>
        <property name="SAVE_ERROR_TITLE"		value="Save Work Error"></property>
        <property name="PROPERTIES_LOAD_ERROR_MESSAGE"	value="An error occured loading a properties file."></property>	
        <property name="PROPERTIES_LOAD_ERROR_TITLE"	value="Properties File Loading Error"></property>
	
        <!-- VERIFICATION MESSAGES -->
	
        <property name="NEW_COMPLETED_MESSAGE"		value="New Work Ready for Editing"></property>
        <property name="NEW_COMPLETED_TITLE"		value="New Work Created"></property>
        <property name="LOAD_COMPLETED_MESSAGE"		value="File Loading Completed and ready for editing"></property>
        <property name="LOAD_COMPLETED_TITLE"		value="Loading Completed"></property>
        <property name="SAVE_COMPLETED_MESSAGE"		value="File Saving Completed"></property>
        <property name="SAVE_COMPLETED_TITLE"		value="Saving Completed"></property>
        <property name="SAVE_UNSAVED_WORK_MESSAGE"	value="Would you like to save your unsaved work first?"></property>
        <property name="SAVE_UNSAVED_WORK_TITLE"	value="Save unsaved work?"></property>
        <property name="EXPORT_CODE_MESSAGE"            value="Code Exported"></property>
        <property name="EXPORT_CODE_COMPLETION"         value="Export to Code Completed"></property>
        <property name="EXPORT_PHOTO_MESSAGE"           value="Photo Exported"></property>
        <property name="EXPORT_PHOTO_COMPLETION"        value="Export to Photo Completed"></property>
	
        <property name="NEW_CREATED_MESSAGE"		value="New Work Created but not yet Saved"></property>
        <property name="WORK_LOADED_MESSAGE"		value="The Work has been Loaded"></property>
        <property name="WORK_SAVED_MESSAGE"		value="The Work has been Saved"></property>
        <property name="SAVE_UNSAVED_WORK_MESSAGE"	value="Do you want to save your unsaved work first?"></property>
        <property name="SAVE_WORK_TITLE"		value="Save your work"></property>
        <property name="LOAD_WORK_TITLE"		value="Select work file to load"></property>
        <property name="WORK_FILE_EXT"			value="pm"></property>
        <property name="WORK_FILE_EXT_DESC"		value="Pose Maker Files"></property>
    </property_list>
    <property_options_list>
    </property_options_list>
</properties>